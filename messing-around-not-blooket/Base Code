var choice = prompt('Please select a symbol:\x0aMultiplication 1\x0aDivision: 2\x0aSubtraction: 3\x0aAdditition: 4'),
    c1raw = prompt(_0x3e76cc(0x1d7)),
    c2raw = prompt(_0x3e76cc(0x1cb)),
    c1 = Number(c1raw),
    c2 = Number(c2raw),
    num = Number(choice);
switch (num) {
case 0x1:
    alert(c1 * c2);
    break;
case 0x2:
    alert(c1 / c2);
    break;
case 0x3:
    alert(c1 - c2);
    break;
case 0x4:
    alert(c1 + c2);
    break;
default:
    alert(_0x3e76cc(0x1ca));
}
